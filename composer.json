{
  "name": "desperial/yii2-ticket-support",
  "description": "Yii2 Support Ticket Module, easy, flexible, fast. And with fixes of font-awesome",
  "type": "yii2-extension",
  "repositories": [
    {
      "type": "vcs",
      "url": "https://github.com/desperial/yii2-ticket-support"
    }
  ],
  "keywords": [
    "yii2",
    "support",
    "ticket",
    "module"
  ],
  "license": "MIT",
  "authors": [
    {
      "name": "akiraz2",
      "email": "akiraz@bk.ru"
    }
  ],
  "minimum-stability": "dev",
  "prefer-stable": true,
  "require": {
    "yiisoft/yii2": "~2.0",
    "hashids/hashids": "^2.0",
    "yiidoc/yii2-redactor": "*",
    "php-imap/php-imap": "^3.0",
    "yiisoft/yii2-queue": "~2.0",
    "rmrevin/yii2-fontawesome": "*"
  },
  "autoload": {
    "psr-4": {
      "akiraz2\\support\\": ""
    }
  },
  "extra": {
    "bootstrap": "akiraz2\\support\\Bootstrap"
  }
}
